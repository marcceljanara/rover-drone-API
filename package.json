{
  "name": "rover-drone-api",
  "version": "1.0.0",
  "description": "Backend API to support Rover Drone project",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node src/app.js",
    "start:dev": "nodemon src/app.js",
    "test": "jest --setupFiles dotenv/config -i --forceExit",
    "test:watch:change": "jest --watch --setupFiles dotenv/config -i",
    "test:watch": "jest --watchAll --coverage --setupFiles dotenv/config -i",
    "migrate": "node-pg-migrate",
    "migrate:test": "node-pg-migrate -f config/database/test.json",
    "lint": "eslint ./src",
    "lint-fix": "eslint --fix ./src",
    "generate-admin": "node src/utils/generateAdmin.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcceljanara/rover-drone-api.git"
  },
  "keywords": [
    "rover",
    "drone",
    "backend",
    "mqtt",
    "rental",
    "iot"
  ],
  "author": "I Nengah Marccel JBC",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marcceljanara/rover-drone-api/issues"
  },
  "homepage": "https://github.com/marcceljanara/rover-drone-api#readme",
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/preset-env": "^7.26.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^29.7.0",
    "node-pg-migrate": "^7.8.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0"
  },
  "dependencies": {
    "@types/jest": "^29.5.14",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.0.9",
    "nodemailer": "^6.9.16",
    "pg": "^8.13.1"
  }
}
